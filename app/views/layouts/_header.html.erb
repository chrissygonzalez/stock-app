<header>
    <% if signed_in? %> 
    <%=     link_to "Portfolio", user_stocks_path(current_user), class: 'button' %>
    <%=     link_to "Transactions", user_transactions_path(current_user), class: 'button' %>
    <%=     link_to "Log Out", logout_path, method: "delete", class: 'button' %>
    <% else %>
<%= link_to "Sign Up", new_user_path %>
<%= link_to "Sign In", signin_path %>
    <% end %>
</header>